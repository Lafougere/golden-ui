{
  "name": "golden-ui",
  "dependencies": {
    "bootstrap-sass": "~3.3.5"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.2.0",
    "mocha": "~2.2.5"
  },
  "homepage": "https://github.com/Lafougere/golden-ui",
  "authors": [
    "Axel Glorieux <axel.glorieux@gmail.com>"
  ],
  "description": "a bootstrap compatible rpg theme",
  "main": ["app/scripts/main.js","app/styles/main.scss"],
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "css",
    "bootstrap",
    "rpg",
    "theme",
    "golden-ui"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ]
}
